<h1>Exhibits</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>
  <% for exhibit in @exhibit %>
  <tr>
    <td><a href="/exhibit/<%= exhibit.id %>"><%= exhibit.title %></a></td>
  </tr>
  <% end %>
</table>

<form method="get" action="/exhibit/new">
  <input type="submit" value="Add Exhibit"/>
</form>
